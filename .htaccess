# Turn Rewrite Engine On
RewriteEngine on

# Rewrite for test.php

# NC makes the rules non case sensitive
# L makes this the last rule that this specific condition will match

# Rewrite for test.php?bin=xxxxx
RewriteRule ^bin/([0-9a-zA-Z]+) neo.php?bin=$1 [NC,L]

RewriteRule ^notifdiscord/([0-9a-zA-Z]+) botnotifneo.php?subject=$1 [NC,L]


